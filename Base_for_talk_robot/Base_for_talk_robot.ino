void sayRandomSentanceIam(int randomlyGeneratedCaseNumber){
  switch (randomlyGeneratedCaseNumber) {
    case 0:    // I am [Adjective]
      sayIam();
      voice.say(adjectives[getRandomNumber(adjectivesSize)]);
      break;
    case 1:    //I am [Adjective][conjunction][determinant][noun]
      sayIam();
      voice.say(adjectives[getRandomNumber(adjectivesSize)]);
      voice.say(conjunctions[getRandomNumber(conjunctionsSize)]);
      voice.say(determinants[getRandomNumber(determinantsSize)]);
      voice.say(nouns[getRandomNumber(nounsSize)]);  
      break;
    case 2:    // I am [Adjective][conjunction][Adjective]
      sayIam();
      voice.say(adjectives[getRandomNumber(adjectivesSize)]);
      voice.say(conjunctions[getRandomNumber(conjunctionsSize)]);
      voice.say(adjectives[getRandomNumber(adjectivesSize)]);
      break;
    case 3:   // I am [Determinant][noun]
      sayIam();
      voice.say(determinants[getRandomNumber(determinantsSize)]);
      voice.say(nouns[getRandomNumber(nounsSize)]);  
      break;
    case 4:   //I am [Determinant][Preposition][noun]
      sayIam();
      voice.say(determinants[getRandomNumber(determinantsSize)]);
      voice.say(prepositions[getRandomNumber(prepositionsSize)]);
      voice.say(nouns[getRandomNumber(nounsSize)]);  
      break;
    case 5:   // I am [Determinant][Preposition][adjective][noun]
      sayIam();
      voice.say(determinants[getRandomNumber(determinantsSize)]);
      voice.say(prepositions[getRandomNumber(prepositionsSize)]);
      voice.say(adjectives[getRandomNumber(adjectivesSize)]);
      voice.say(nouns[getRandomNumber(nounsSize)]);
      break;
    case 6:   // I am [pronoun][noun]
      sayIam();
      voice.say(pronouns[getRandomNumber(pronounsSize)]);
      voice.say(nouns[getRandomNumber(nounsSize)]);  
      break;          
    case 7:   //  I am [Pronomen][Adjektiv][Substantiv]
      sayIam();
      voice.say(pronouns[getRandomNumber(pronounsSize)]);
      voice.say(adjectives[getRandomNumber(adjectivesSize)]);
      voice.say(nouns[getRandomNumber(nounsSize)]);  
      break;  
  }
}

