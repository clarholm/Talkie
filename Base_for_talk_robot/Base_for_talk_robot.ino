      voice.say(nouns[getRandomNumber(nounsSize)]);  
      break;
    case 3:   // I [Verb][Adverb][Determinant][Adjektiv][Substantiv]
      sayI();
      voice.say(verbs[getRandomNumber(verbsSize)]);
      voice.say(adverbs[getRandomNumber(adverbsSize)]);      
